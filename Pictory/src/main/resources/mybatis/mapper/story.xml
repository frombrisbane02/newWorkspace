<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="mybatis.mapper.story">	

	<select id="storyllist" resultType="com.pictory.springapp.dto.StoryDTO">
		SELECT * FROM STORY
	</select>
	
	<!-- 스토리가 #번인 사진 전부 가져오기 -->

	<select id="storyimages" resultType="com.pictory.springapp.dto.StoryDTO" parameterType="int">
	<!--select photoName from photo where postno in (select postno from post where sNO=#{sNo}) -->
	select photoName from photo where postno in (select postno from post where sNO=1)
	</select> 

	
	

</mapper>